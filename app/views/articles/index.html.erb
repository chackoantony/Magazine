<h1>Articles</h1>
<%= link_to 'New Article', new_article_path %>

<table>
  <thead>
    <th>Title</th>
    <th>Actions</th>
  </thead>
  <tbody>
  <% @articles.each do |article| %>
    <tr>
      <td><%= article.title %></td>
      <td>
        <%= link_to 'Show', article_path(article) %>
        <%= link_to 'Delete', article_path(article), method: 'delete', data: { confirm: 'You sure?' } %>
      </td>
    </tr>  
    <% end %>
  </tbody>
</table>
